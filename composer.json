{
    "name": "anaxago/symfony2-coding-standard",
    "type": "phpcodesniffer-standard",
    "description": "CodeSniffer ruleset for the Symfony 2+ coding standard - forked from https://github.com/djoos/Symfony-coding-standard",
    "keywords": ["Symfony2", "Symfony", "coding standard", "phpcs"],
    "homepage": "https://github.com/Anaxago/Symfony-coding-standard",
    "license": "MIT",
    "extra": {
        "branch-alias": {
            "dev-master": "3.x-dev"
        }
    },
    "minimum-stability": "dev",
    "require-dev": {
        "squizlabs/php_codesniffer": "3.*",
        "phpunit/phpunit": "^4.0 || ^5.0 || ^6.0"
    },
    "conflict": {
        "squizlabs/php_codesniffer": "<3 || >=4"
    }
}
